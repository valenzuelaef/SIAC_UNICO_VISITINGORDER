<div class="col-md-12 column-layout">
    <div class="bg-container">
        <!--Title-->
        <div class="customer-data-title box title-box">
            <div class="customer-data-title-items customer-data-title-icon" style="width: 5%; padding: 0">
                <span class="package-icon-VisitingOrder rounded-circle"></span>
            </div>
            <div class="customer-data-title-items customer-data-title-label" style="padding-top: 10px; width: 60%">
                <span class="panel-titulo-cp">DIRECCIÓN DE INSTALACIÓN</span>
            </div>
        </div>
        <!--End Title-->

        <div class="col-md-9">
            <label class="label-cp">Dirección</label>
            <p id="txtAddress" class="input-customer"></p>
        </div>

        <div class="col-md-3">
            <label class="label-cp">Notas de Dirección</label>
            <p id="txtAddressNote" class="input-customer"></p>
        </div>

        <div class="clearfix"></div>

        <div class="col-md-3">
            <label class="label-cp">País</label>
            <p id="txtCountry" class="input-customer"></p>
        </div>
        <div class="col-md-3">
            <label class="label-cp">Departamento</label>
            <p id="txtDepartment" class="input-customer"></p>
        </div>
        <div class="col-md-3">
            <label class="label-cp">Provincia</label>
            <p id="txtProvince" class="input-customer"></p>
        </div>
        <div class="col-md-3">
            <label class="label-cp">Distrito</label>
            <p id="txtDistrict" class="input-customer"></p>
        </div>
        <div class="clearfix"></div>

        <div class="col-md-3">
            <label class="label-cp">Código de Plano</label>
            <p id="txtPlane" class="input-customer"></p>
        </div>
        <div class="col-md-3">
            <label class="label-cp">Código de Ubigeo</label>
            <p id="txtUbigeo" class="input-customer"></p>
        </div>
        <div class="col-md-3">
            <span>&nbsp;</span>
        </div>
        <div class="col-md-3">
            <span>&nbsp;</span>
        </div>
        <div class="clearfix"></div>

    </div>
</div>
@Scripts.Render("~/bundles/Content/Lib/BloqueoF12")